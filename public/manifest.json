{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Up Next",
    "action": {
        "default_title": "Click to open panel"
    },
    "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
    "side_panel": {
        "default_path": "index.html"
    },
    "permissions": [
        "scripting",
        "sidePanel",
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "32": "icon-32.png"
    }
}