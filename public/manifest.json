{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Up Next",
    "action": {
        "default_title": "Click to open panel"
    },
    "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "commands": {
    "save_tab": {
      "suggested_key": "Alt+Shift+Period",
      "description": "Add a tab to up next"
    },
    "open_sidebar": {
      "suggested_key": "Alt+Shift+Comma",
      "description": "Open the sidebar"
    }
},
    "side_panel": {
        "default_path": "index.html"
    },
    "permissions": [
        "scripting",
        "sidePanel",
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "32": "icon-32.png"
    }
}